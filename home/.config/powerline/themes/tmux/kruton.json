{
	"default_module": "powerline.segments.common",
	"segments": {
		"right": [
			{
				"function": "powerline.segments.common.sys.system_load",
				"priority": 50
			},
			{
				"function": "powerline.segments.common.time.date",
				"args": {
				       "format": "%b %d"
				}
			},
			{
				"function": "powerline.segments.common.time.date",
				"name": "time",
				"args": {
					"format": "%H:%M",
					"istime": true
				}
			}
		]
	},
	"# vim: set noet ts=8 sts=0 sw=8 :": null
}
